{
  "name": "accounting-system",
  "version": "1.0.0",
  "description": "A money accounting system",
  "main": "index.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "client-start": "cd client && npm start",
    "api-install": "cd api && npm install",
